{
  "name": "express-composition",
  "version": "1.0.0",
  "description": "Useful complement for `express` applications.",
  "main": "src/index.js",
  "scripts": {
    "clean:modules": "rimraf node_modules",
    "clean:coverage": "rimraf coverage .nyc_output",
    "docs": "javadoc -i src/*.js -o docs/README.api.md -f markdown && concat docs/README.ini.md docs/README.api.md docs/README.end.md -o README.md",
    "test": "mocha test/*.test.js",
    "cover": "npm run clean:coverage && nyc --reporter=html mocha test/*.test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "concat": "^1.0.3",
    "javadoc": "^2.0.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "request": "^2.88.0",
    "rimraf": "^3.0.1"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "import-fresh": "^3.2.1"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allnulled/express-composition.git"
  },
  "bugs": {
    "url": "https://github.com/allnulled/express-composition/issues"
  },
  "homepage": "https://github.com/allnulled/express-composition#readme"
}
